type Grade {
  id: ID!
  name: String!
  institution: Institution!
  subjects: [ Subject! ]!
}

input GradeCreationInput {
  institutionId: ID!
  name: String!
}

extend type Query {
  findGrade (input: ID!) : Grade
  findGradesByInstitution(institutionId: ID!): [ Grade! ]!
}

extend type Mutation {
  createGrade (input : GradeCreationInput!) : Grade
}