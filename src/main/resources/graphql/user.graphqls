type User {
  id: ID!
  username: String!
  email: String!
}

input UserBean {
  username: String!
  email: String!
  password: String!
}

input AuthenticationBean {
  email: String!
  password: String!
}

type Query {
  login(auth: AuthenticationBean!) : String!
  findUser(id: ID!): User
  findLoggedUser: User!
}

type Mutation {
  createUser(userBean: UserBean!) : User
}